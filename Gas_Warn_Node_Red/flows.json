    {
        "id": "afb2edafb819389a",
        "type": "ui_template",
        "z": "63b20dee50f75889",
        "group": "9c7b00b28e659be2",
        "name": "",
        "order": 2,
        "width": 2,
        "height": 2,
        "format": "<div id=\"circle\" style=\"\n    width: 50px; \n    height: 50px; \n    border-radius: 50%; \n    margin: auto; \n    background-color: green;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 20px;\n    color: white;\n    text-align: center;\n    font-weight: bold;\">\n    0%\n</div>\n<script>\n    (function(scope) {\n    let blinkInterval = null;\n\n    scope.$watch('msg.payload', function(data) {\n        var circle = document.getElementById('circle');\n        let percentage = parseInt(data, 10);\n\n        // Ensure data is a valid percentage\n        if (!isNaN(percentage) && percentage >= 0 && percentage <= 100) {\n            circle.textContent = `${percentage}%`;\n\n            if (percentage > 40) {\n                // Start blinking effect\n                if (!blinkInterval) {\n                    blinkInterval = setInterval(() => {\n                        circle.style.backgroundColor = \n                            circle.style.backgroundColor === 'red' ? 'transparent' : 'red';\n                    }, 500);\n                }\n            }\n            else if(percentage > 10){\n                if (blinkInterval) {\n                clearInterval(blinkInterval);\n                blinkInterval = null;\n                }\n                circle.style.backgroundColor = 'yellow';\n            }\n            else {\n                // Stop blinking and reset background color\n                if (blinkInterval) {\n                    clearInterval(blinkInterval);\n                    blinkInterval = null;\n                }\n                circle.style.backgroundColor = 'green';\n            }\n        }\n    });\n})(scope);\n</script>",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "resendOnRefresh": true,
        "templateScope": "local",
        "className": "",
        "x": 360,
        "y": 160,
        "wires": [
            []
        ]
    },
    {
        "id": "78748910d848b574",
        "type": "function",
        "z": "63b20dee50f75889",
        "name": "function 1",
        "func": "// Lấy giá trị từ input\nvar value = msg.payload;\n// Kiểm tra giá trị\nif (value > 40) {\n    // Lưu trạng thái cảnh báo\n    flow.set(\"alerting\", true);\n    msg.payload = \"Cảnh báo: Giá trị vượt quá 40%!\";\n    return msg;\n} else {\n    // Nếu giá trị dưới 40%, dừng cảnh báo\n    flow.set(\"alerting\", false);\n    return null;\n}\n",
